{
	"family": "jenkins-demo-containers",
	"taskRoleArn": "arn:aws:iam::101262499873:role/ecsInstanceRole",
	"executionRoleArn": "arn:aws:iam::101262499873:role/ecsInstanceRole",
	"networkMode": "bridge",
	"containerDefinitions": [{
		"name": "jenkins-demo-containers",
		"image": "101262499873.dkr.ecr.eu-central-1.amazonaws.com/jenkins-repo:latest",
		"portMappings": [{
			"containerPort": 8080,
			"hostPort": 8080
		}],
		"essential": true,
		"entryPoint": [
			""
		],
		"command": [
			""
		],
		"environment": [{
			"name": "javaenv",
			"value": "prod"
		}],
		"disableNetworking": true,
		"privileged": true,
		"readonlyRootFilesystem": true,
		"dnsServers": [
			""
		],
		"dnsSearchDomains": [
			""
		],
		"extraHosts": [{
			"hostname": "",
			"ipAddress": ""
		}],
		"dockerSecurityOptions": [
			""
		],
		"dockerLabels": {
			"KeyName": ""
		},
		"ulimits": [{
			"name": "rss",
			"softLimit": 0,
			"hardLimit": 0
		}],
		"logConfiguration": {
			"logDriver": "awslogs",
			"options": {
				"KeyName": ""
			}
		}
	}]
	"placementConstraints": [{
		"type": "memberOf",
		"expression": "attribute:ecs.instance-type =~ t2.*"
	}],
	"requiresCompatibilities": [
		"EC2"
	],
	"memory": "512"
}
