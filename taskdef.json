{
  "family": "jenkins-demo-containers",
  "containerDefinitions": [
    {
      "name": "Jenkins-demo",
      "image": "101262499873.dkr.ecr.eu-central-1.amazonaws.com/jenkins-repo:latest",
      "cpu": 10,
      "memory": 300,
      "portMappings": [
          {
              "containerPort": 8080,
              "hostPort": 8080
          }
      ],
      "essential": true,
      "environment": [
          {
              "name": "NODE_ENV",
              "value": "production"
          }
      ]
    }
  ]
}
